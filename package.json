{
  "name": "@scoreboardmax/api-types",
  "version": "1.0.10",
  "description": "TypeScript definitions for ScoreboardMax API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run copy-types",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs && mv dist/cjs/index.js dist/index.cjs",
    "build:esm": "tsc --module es6 --outDir dist/esm && mv dist/esm/index.js dist/index.mjs",
    "copy-types": "cp index.d.ts dist/index.d.ts",
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "echo \"No tests for type definitions\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScoreboardMax/scoreboardmax-api-types.git"
  },
  "keywords": [
    "typescript",
    "api",
    "scoreboardmax",
    "types",
    "definitions",
    "scoreboard",
    "sports"
  ],
  "author": "ScoreboardMax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ScoreboardMax/scoreboardmax-api-types/issues"
  },
  "homepage": "https://scoreboardmax.com/api/docs",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
